{
  "version": 2,
  "builds": [
    {
      "src": "game/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/game/index.html"
    },
    {
      "src": "/game/(.*)",
      "dest": "/game/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/game/$1"
    }
  ]
}
